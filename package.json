{
  "name": "PixelHunter",
  "version": "5.0.0",
  "private": true,
  "description": "Личный проект «Пиксель хантер» от HTML Academy",
  "scripts": {
    "lint:commit": "commitlint-travis",
    "lint:js": "eslint ./gulpfile.js js/",
    "lint:scss": "stylelint 'sass/**/*.scss' --syntax scss",
    "build": "gulp build",
    "start": "gulp serve",
    "test": "gulp test",
    "ci:travis": "npm-run-all -c -p --aggregate-output lint:* test build",
    "ci": "npm-run-all -c -p --aggregate-output lint:js lint:scss test build"
  },
  "devDependencies": {
    "@commitlint/cli": "7.0.0",
    "@commitlint/config-conventional": "7.0.1",
    "@commitlint/prompt-cli": "7.0.0",
    "@commitlint/travis-cli": "7.0.0",
    "autoprefixer": "9.0.2",
    "browser-sync": "2.24.6",
    "chai": "4.1.2",
    "cross-env": "5.2.0",
    "css-mqpacker": "7.0.0",
    "del": "3.0.0",
    "eslint": "4.19.1",
    "eslint-config-htmlacademy": "0.5.x",
    "gulp": "3.9.1",
    "gulp-better-rollup": "3.3.0",
    "gulp-csso": "3.0.1",
    "gulp-imagemin": "4.1.0",
    "gulp-mocha": "6.0.0",
    "gulp-plumber": "1.2.0",
    "gulp-postcss": "7.0.1",
    "gulp-rename": "1.4.0",
    "gulp-sass": "4.0.1",
    "gulp-sourcemaps": "2.6.4",
    "gulp-svgstore": "6.1.1",
    "husky": "1.0.0-rc.13",
    "npm-run-all": "4.1.3",
    "prettier": "1.14.2",
    "prettier-eslint": "8.8.2",
    "rollup-plugin-commonjs": "9.1.5",
    "stylelint": "9.4.x",
    "stylelint-config-htmlacademy": "0.1.x"
  },
  "engines": {
    "node": "8"
  }
}
